{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist/ && tsc",
    "dev": "nodemon",
    "dev:command": "npm run build && npm run start:ts",
    "lint": "pretty-quick --pattern '**/*.ts' --verbose && eslint ./src --fix",
    "start": "node ./dist/index.js",
    "start:ts": "ts-node ./src/index.ts"
  },
  "keywords": [
    "mysql",
    "typeorm",
    "express"
  ],
  "author": "Daniel Solarte Chaverra",
  "license": "MIT",
  "_moduleAliases": {
